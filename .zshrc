alias update-dusty='scp -i ~/Downloads/my-new-key.pem -r ~/Desktop/dusty-thrifts-site/* ec2-user@98.86.78.124:/tmp/ && ssh -i ~/Downloads/my-new-key.pem ec2-user@98.86.78.124 "sudo mkdir -p /var/backups && sudo cp -r /var/www/html /var/backups/html-$(date +%Y%m%d-%H%M%S) && sudo rm -rf /var/www/html/* && sudo mv /tmp/* /var/www/html/ && sudo chown -R apache:apache /var/www/html/ && sudo chmod -R 755 /var/www/html/"'
